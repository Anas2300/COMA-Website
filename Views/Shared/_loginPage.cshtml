@model IEnumerable<MusicianApp.Models.Person>

@if ((string) ViewData["memberStatus"] == "1")
{
<form asp-area="Identity" asp-page="/Account/Logout" method="post" id="logoutForm" class="navbar-right">
    <ul class="nav navbar-nav navbar-right">
        <li>
            <a asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Hello !</a>
        </li>
        <li>
            <button type="submit" class="btn btn-link navbar-btn navbar-link">Logout</button>
        </li>
    </ul>
</form>
}
else
{
<ul class="nav navbar-nav navbar-right">
    <li><a asp-controller="Members" asp-action="Create">Register</a></li>
    <li><a asp-controller="Login" asp-action="Create">Login</a></li>
</ul>
}